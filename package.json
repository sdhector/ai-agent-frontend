{
  "name": "ai-agent-frontend",
  "version": "1.0.0",
  "description": "AI Agent Frontend - React Native PWA & Android APK",
  "main": "expo-router/entry",
  "scripts": {
    "start": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "build:web": "npx expo export --platform web",
    "build:web:production": "npx expo export --platform web",
    "build:android": "eas build --platform android --profile production",
    "build:android:preview": "eas build --platform android --profile preview",
    "deploy:web": "./scripts/deploy-pwa.sh",
    "preview:web": "npx serve dist -p 3000",
    "audit": "lighthouse http://localhost:3000 --view --preset=desktop",
    "lint": "eslint .",
    "type-check": "tsc --noEmit",
    "test": "jest",
    "clean": "rm -rf dist .expo"
  },
  "dependencies": {
    "@expo/vector-icons": "^14.0.0",
    "@react-native-async-storage/async-storage": "~2.1.0",
    "@react-navigation/bottom-tabs": "^7.1.0",
    "@react-navigation/native": "^7.0.0",
    "@react-navigation/native-stack": "^7.1.0",
    "ajv": "^8.17.1",
    "expo": "~52.0.0",
    "expo-auth-session": "~6.0.0",
    "expo-clipboard": "~7.0.0",
    "expo-constants": "~17.0.0",
    "expo-crypto": "~14.0.0",
    "expo-linking": "~7.0.0",
    "expo-router": "~4.0.0",
    "expo-secure-store": "~14.0.0",
    "expo-status-bar": "~2.0.0",
    "expo-web-browser": "~14.0.0",
    "nativewind": "^4.1.23",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "react-native": "0.76.5",
    "react-native-markdown-display": "^7.0.2",
    "react-native-safe-area-context": "~4.12.0",
    "react-native-screens": "~4.4.0",
    "react-native-web": "^0.19.13",
    "react-native-worklets": "^0.6.1"
  },
  "devDependencies": {
    "@babel/core": "^7.26.0",
    "@testing-library/react-native": "^12.8.3",
    "@types/react": "~18.2.79",
    "@types/react-native": "~0.73.0",
    "@typescript-eslint/eslint-plugin": "^8.18.1",
    "@typescript-eslint/parser": "^8.18.1",
    "eslint": "^9.17.0",
    "jest": "^29.7.0",
    "prettier": "^3.4.2",
    "tailwindcss": "^3.4.17",
    "typescript": "^5.7.2"
  },
  "private": true
}
