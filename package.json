{
  "name": "ai-agent-frontend",
  "version": "1.0.0",
  "description": "AI Agent Frontend - React Native PWA & Android APK",
  "main": "index.js",
  "scripts": {
    "postinstall": "patch-package",
    "prestart": "npm run clean:cache",
    "start": "expo start --clear",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "build:web": "npx expo export --platform web",
    "build:web:production": "npx expo export --platform web",
    "build:android": "eas build --platform android --profile production",
    "build:android:preview": "eas build --platform android --profile preview",
    "deploy:web": "./scripts/deploy-pwa.sh",
    "preview:web": "npx serve dist -p 3000",
    "audit": "lighthouse http://localhost:3000 --view --preset=desktop",
    "lint": "eslint .",
    "type-check": "tsc --noEmit",
    "test": "jest",
    "clean": "rm -rf dist .expo",
    "clean:cache": "node -e \"const fs = require('fs'); const path = require('path'); const dirs = ['.expo', 'node_modules/.cache', '.cache']; dirs.forEach(d => { try { fs.rmSync(d, {recursive: true, force: true}); console.log('Cleaned:', d); } catch(e) {} });\""
  },
  "dependencies": {
    "@expo/vector-icons": "^15.0.3",
    "@react-navigation/bottom-tabs": "^7.1.0",
    "@react-navigation/native": "^7.0.0",
    "@react-navigation/native-stack": "^7.1.0",
    "ajv": "^8.17.1",
    "expo": "~54.0.0",
    "expo-auth-session": "~7.0.9",
    "expo-clipboard": "~8.0.7",
    "expo-constants": "~18.0.10",
    "expo-crypto": "~15.0.7",
    "expo-font": "^14.0.9",
    "expo-linking": "~8.0.9",
    "expo-splash-screen": "~31.0.11",
    "expo-status-bar": "~3.0.8",
    "expo-web-browser": "~15.0.9",
    "nativewind": "^4.1.23",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-native": "0.81.5",
    "react-native-markdown-display": "^7.0.2",
    "react-native-mmkv": "^3.3.3",
    "react-native-reanimated": "^4.1.5",
    "react-native-safe-area-context": "~5.6.0",
    "react-native-screens": "~4.16.0",
    "react-native-web": "^0.21.0"
  },
  "devDependencies": {
    "@babel/core": "^7.26.0",
    "@types/react": "~19.1.10",
    "@types/react-native": "~0.73.0",
    "@typescript-eslint/eslint-plugin": "^8.18.1",
    "@typescript-eslint/parser": "^8.18.1",
    "eslint": "^9.17.0",
    "jest": "^29.7.0",
    "patch-package": "^8.0.1",
    "prettier": "^3.4.2",
    "react-native-worklets": "^0.6.1",
    "sharp": "^0.34.5",
    "tailwindcss": "^3.4.17",
    "typescript": "^5.7.2"
  },
  "private": true
}
